{
  "pipeline": {
    "build": {
      "outputs": ["dist/**", ".next/**", "public/dist/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "compile": {
      "outputs": ["dist/**", ".next/**", "public/dist/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "package": {
      "outputs": ["dist/**", ".next/**", "public/dist/**", "build/**"],
      "dependsOn": ["^build"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": []
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": ["^build"]
    },
    "ladle": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
